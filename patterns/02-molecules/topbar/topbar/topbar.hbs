<div class="topbar">
    <div class="container">
        <div class="row">
            <div class="col-xs-12">
                <div class="topbar__inner">
                    <div class="topbar__logo">
                        {{ render '@logo' }}
                    </div>
                    <div class="topbar__navigation">
                    <div class="topbar__navigation-secondary">
                        More at {{ render '@link' githubLink }}
                    </div>
                    <div class="topbar__navigation-primary">
                        {{ render '@nav-main-list' navMainList }}
                        <div class="topbar__navigation-search">
                        {{ render '@form-field--search' search }}
                        </div>
                    </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
